    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@types/axios": "^0.14.0",
    "@types/jest": "^23.3.2",
    "@types/react": "^16.4.14",
    "@types/styled-components": "^3.0.1",
    "axios": "^0.18.0",
    "axios-mock-adapter": "^1.15.0",
    "babel": "^6.23.0",
    "babel-loader": "^8.0.4",
    "cross-env": "^5.2.0",
    "gulp": "^3.9.1",
