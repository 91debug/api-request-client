    "gulp-typescript": "^5.0.0-alpha.3",
    "jest": "^23.6.0",
    "merge2": "^1.2.2",
    "prettier": "^1.14.3",
    "run-sequence": "^2.2.1",
    "ts-jest": "^23.10.2",
